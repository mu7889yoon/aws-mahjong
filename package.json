{
  "name": "aws-mahjong-tiles",
  "version": "1.0.0",
  "description": "AWS-themed Mahjong tile SVG/PNG generator with PDF layout support",
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "generate": "ts-node src/cli.ts",
    "pdf:layout": "ts-node src/png-layout-pdf-cli.ts",
    "docs:build": "node scripts/build-docs.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist output"
  },
  "keywords": [
    "mahjong",
    "aws",
    "svg",
    "tiles",
    "generator"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "fast-check": "^3.15.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@resvg/resvg-js": "^2.6.2"
  }
}
